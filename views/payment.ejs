<%- include('header'); -%>
    <div class="container-saving">
        <div class="wrap-saving">
            <form class="saving-form validate-form" action="" method="POST">
                <span class="saving-form-title">
                    Nộp tiền vào tài khoản
            </span>
                <div class="wrap-input100 validate-input" data-validate="">
                    <label class="label-input100" for=" ">Tài khoản: <b style="text-transform:uppercase;"> le ho gia kiet</b></label>
                </div>
                <div class="wrap-input100 validate-input " data-validate=" ">
                    <label class="label-input100 " for=" ">STK: 0000 0000 0000</label>
                </div>
                <div class="wrap-input100 validate-input" data-validate="">
                    <input class="input100" type="number" id="" name="" placeholder="Số tiền cần nộp">
                    <span class="focus-input100"></span>
                </div>
                <div class="container-saving-form-btn">
                    <button class="saving-form-btn">
                        Nộp tiền
                    </button>
                </div>
            </form>
        </div>
    </div>

